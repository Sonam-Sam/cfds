    <div class="container col-md-8 text-center">
      <h3>Register Your Account</h3>
      <form action="/register" method="POST" class="py-5">
        <div class="row">
          <div class="form-floating col-md-6 mt-3 ml-auto gx-1">
            <input
            name="stdid"
              type="number"
              class="form-control"
              id="floatingInput"
              placeholder="name@example.com"
            />
            <label for="floatingInput">Student ID</label>
          </div>
          <div class="form-floating col-md-6 mt-3 ml-auto gx-1">
            <input
            name="email"
              type="email"
              class="form-control"
              id="floatingPassword"
              placeholder="name@example.com"
            />
            <label for="floatingPassword">Email</label>
          </div>
          <div class="form-floating col-md-6 mt-3 ml-auto gx-1">
            <input
            name="name"
              type="text"
              class="form-control"
              id="floatingInput"
              placeholder="name@example.com"
            />
            <label for="floatingInput">Name</label>
          </div>
          <div class="form-floating col-md-6 mt-3 ml-auto gx-1">
            <input
              type="tel"
              name="number"
              class="form-control"
              id="floatingPassword"
              placeholder="name@example.com"
            />
            <label for="floatingPassword">Phone Number</label>
          </div>
          <div class="form-floating col-md-6 mt-3 ml-auto gx-1">
            <input
            name="password"
              type="password"
              class="form-control"
              id="floatingInput"
              placeholder="name@example.com"
            />
            <label for="floatingInput">Password</label>
          </div>
          <div class="form-floating col-md-6 mt-3 ml-auto gx-1">
            <input
            name="confirmPassword"
              type="password"
              class="form-control"
              id="floatingPassword"
              placeholder="name@example.com"
            />
            <label for="floatingPassword">Confirm Password</label>
          </div>
        </div>

        <button type="submit" class="btn btn-cfds mt-5">Register</button>
      </form>
      <p>
        Already have an account? <a href="/login" class="linkc">Log In</a>
      </p>
    </div>

    <% if (typeof errorMessage !=="undefined" ) { %>
      <script>
        Swal.fire({
          icon: 'error',
          title: 'Oops...',
          text: "<%= errorMessage %>",
        })
        </script>
    <% } %>
    <% if (typeof successMessage !== "undefined") { %>
      <script>
        swal({
          title: "Good job!",
          text: "<%= successMessage %>",
          icon: "success",
        });
        </script>
    <% } %>
  
  </body>
